{
  "name": "@atlaspack/reporter-dev-server",
  "version": "2.14.24",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "main": "./lib/ServerReporter.js",
  "source": "./src/ServerReporter.ts",
  "types": "./lib/types/ServerReporter.d.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "@atlaspack/plugin": false,
        "@atlaspack/utils": false
      }
    }
  },
  "dependencies": {
    "@atlaspack/plugin": "2.14.24",
    "@atlaspack/utils": "2.18.1",
    "@atlaspack/types": "2.15.14",
    "connect": "^3.7.0",
    "ejs": "^3.1.6",
    "fresh": "^0.5.2",
    "http-proxy-middleware": "^2.0.1",
    "launch-editor": "^2.3.0",
    "mime-types": "2.1.18",
    "nullthrows": "^1.1.1",
    "serve-handler": "^6.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@atlaspack/babel-preset": "2.14.2",
    "@types/connect": "^3.4.38",
    "@types/ejs": "^3.1.5",
    "@types/fresh": "^0.5.3",
    "@types/serve-handler": "^6.1.4"
  },
  "scripts": {
    "check-ts": "tsc --emitDeclarationOnly --rootDir src",
    "build:lib": "gulp build --gulpfile ../../../gulpfile.js --cwd ."
  }
}
