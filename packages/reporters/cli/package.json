{
  "name": "@atlaspack/reporter-cli",
  "version": "2.17.17",
  "license": "(MIT OR Apache-2.0)",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "main": "./lib/CLIReporter.js",
  "source": "./src/CLIReporter.ts",
  "types": "./src/CLIReporter.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "@atlaspack/plugin": false,
        "@atlaspack/types": false,
        "@atlaspack/utils": false,
        "chalk": false,
        "term-size": false
      }
    }
  },
  "dependencies": {
    "@atlaspack/plugin": "workspace:*",
    "@atlaspack/types": "workspace:*",
    "@atlaspack/utils": "workspace:*",
    "chalk": "^4.1.0",
    "filesize": "^6.1.0",
    "nullthrows": "^1.1.1",
    "ora": "^5.2.0",
    "string-width": "^4.2.0",
    "term-size": "^2.2.1",
    "wrap-ansi": "^7.0.0"
  },
  "devDependencies": {
    "@atlaspack/feature-flags": "workspace:*"
  },
  "type": "commonjs",
  "scripts": {
    "build:lib": "gulp build --gulpfile ../../../gulpfile.js --cwd ."
  }
}
