  "dependencies": {
    "@atlaspack/core": "*",
    "@atlaspack/cache": "*",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "commander": "^7.0.0",
    "supertest": "^7.1.1",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.19",
    "@types/node": "^22.13.10",
