{
  "name": "@atlaspack/inspector",
  "version": "2.16.0",
  "license": "(MIT OR Apache-2.0)",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "tsc --project . && yarn workspace @atlaspack/inspector-frontend build",
