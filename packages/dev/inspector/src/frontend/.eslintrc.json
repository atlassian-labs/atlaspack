{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "extends": ["eslint:recommended", "react-app"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/jsx-no-bind": "off",
    "no-console": "off",
    "no-use-before-define": "off"
  },
  "globals": {
    "globalThis": "readonly"
  }
}
