{
  "name": "@atlaspack/utils",
  "version": "3.0.3",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "main": "./lib/index.js",
  "source": "./src/index.ts",
  "types": "./src/index.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "@atlaspack/codeframe": false,
        "@atlaspack/feature-flags": false,
        "@atlaspack/diagnostic": false,
        "@atlaspack/rust": false,
        "@atlaspack/logger": false,
        "@atlaspack/markdown-ansi": false,
        "@parcel/source-map": false,
        "chalk": false
      }
    }
  },
  "dependencies": {
    "@atlaspack/codeframe": "workspace:*",
    "@atlaspack/diagnostic": "workspace:*",
    "@atlaspack/feature-flags": "workspace:*",
    "@atlaspack/logger": "workspace:*",
    "@atlaspack/markdown-ansi": "workspace:*",
    "@atlaspack/rust": "workspace:*",
    "@atlaspack/types-internal": "workspace:*",
    "@parcel/source-map": "^2.1.1",
    "@types/micromatch": "^4.0.9",
    "@types/node-forge": "^1.3.13",
    "ansi-html-community": "0.0.8",
    "chalk": "^4.1.0",
    "clone": "^2.1.1",
    "fast-glob": "^3.2.12",
    "fastest-levenshtein": "^1.0.16",
    "is-glob": "^4.0.0",
    "is-url": "^1.2.2",
    "json5": "^2.2.0",
    "lru-cache": "^6.0.0",
    "micromatch": "^4.0.4",
    "node-forge": "^1.2.1",
    "nullthrows": "^1.1.1",
    "open": "^7.0.3",
    "snarkdown": "^2.0.0",
    "strip-ansi": "^6.0.0",
    "terminal-link": "^2.1.1"
  },
  "devDependencies": {
    "@atlaspack/babel-register": "workspace:*",
    "@types/is-glob": "^4.0.1",
    "benny": "^3.7.1",
    "random-int": "^1.0.0"
  },
  "browser": {
    "./src/generateCertificate.js": false,
    "./src/http-server.js": false,
    "./src/openInBrowser.js": false,
    "@atlaspack/markdown-ansi": false
  },
  "type": "commonjs",
  "scripts": {
    "build:lib": "gulp build --gulpfile ../../../gulpfile.js --cwd ."
  }
}
