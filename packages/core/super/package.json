{
  "name": "@atlaspack/super",
  "version": "2.14.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "build": "rm -rf lib && yarn generate-entries && yarn build:atlaspack && yarn create-empty-files && yarn copy-types",
    "generate-entries": "rm -rf entries && node ./scripts/generate-entries.js",
    "build:atlaspack": "NODE_ENV=production ATLASPACK_BUILD_ENV=production ATLASPACK_SUPER_BUILD=true yarn atlaspack build --no-cache --no-autoinstall ./entries/*.js --config .parcelrc",
    "create-empty-files": "touch lib/core/core/_empty.js && touch lib/utils/node-resolver-core/_empty.js",
    "copy-types": "node ./scripts/copy-types.mjs"
  },
  "targets": {
    "default": {
      "context": "node",
      "isLibrary": true,
      "distDir": "lib",
      "optimize": false,
      "sourceMap": false,
      "includeNodeModules": {
        "lmdb": false
      }
    }
  },
  "alias": {
    "@swc/wasm": false,
    "@atlaspack/babel-register": false
  },
  "exports": {
    "./*": "./*",
    ".": {
      "default": "./lib/core.js",
      "types": "./types/core/core/index.d.ts"
    },
    "./babel-plugin-transform-contextual-imports": "./lib/babel-plugin-transform-contextual-imports.js",
    "./babel-plugin-transform-runtime": "./lib/babel-plugin-transform-runtime.js",
    "./babel-preset-env": "./lib/babel-preset-env.js",
    "./build-cache": "./lib/build-cache.js",
    "./bundler-default": "./lib/bundler-default.js",
    "./bundler-library": "./lib/bundler-library.js",
    "./cache": {
      "default": "./lib/cache.js",
      "types": "./types/core/cache/index.d.ts"
    },
    "./conditional-import-types": {
      "default": "./dummy.js",
      "types": "./types/core/conditional-import-types/index.d.ts"
    },
    "./cli": "./lib/cli.js",
    "./codeframe": "./lib/codeframe.js",
    "./compressor-brotli": "./lib/compressor-brotli.js",
    "./compressor-gzip": "./lib/compressor-gzip.js",
    "./compressor-raw": "./lib/compressor-raw.js",
    "./config-default": "./lib/config-default.js",
    "./config-webextension": "./config-webextension.js",
    "./core": {
      "default": "./lib/core.js",
      "types": "./types/core/core/index.d.ts"
    },
    "./diagnostic": {
      "default": "./lib/diagnostic.js",
      "types": "./types/core/diagnostic/lib/diagnostic.d.ts"
    },
    "./domain-sharding": {
      "default": "./lib/domain-sharding.js",
      "types": "./types/utils/domain-sharding/lib/index.d.ts"
    },
    "./events": "./events.js",
    "./feature-flags": {
      "default": "./lib/feature-flags.js",
      "types": "./types/core/feature-flags/lib/types.d.ts"
    },
    "./fs": {
      "default": "./lib/fs.js",
      "types": "./types/core/fs/index.d.ts"
    },
    "./graph": "./lib/graph.js",
    "./logger": "./lib/logger.js",
    "./markdown-ansi": "./lib/markdown-ansi.js",
    "./namer-default": "./lib/namer-default.js",
    "./node-resolver-core": "./lib/node-resolver-core.js",
    "./optimizer-blob-url": "./lib/optimizer-blob-url.js",
    "./optimizer-css": "./lib/optimizer-css.js",
    "./optimizer-cssnano": "./lib/optimizer-cssnano.js",
    "./optimizer-data-url": "./lib/optimizer-data-url.js",
    "./optimizer-htmlnano": "./lib/optimizer-htmlnano.js",
    "./optimizer-image": "./lib/optimizer-image.js",
    "./optimizer-inline-requires": "./lib/optimizer-inline-requires.js",
    "./optimizer-svgo": "./lib/optimizer-svgo.js",
    "./optimizer-swc": "./lib/optimizer-swc.js",
    "./optimizer-terser": "./lib/optimizer-terser.js",
    "./package-manager": {
      "default": "./lib/package-manager.js",
      "types": "./types/core/package-manager/index.d.ts"
    },
    "./packager-css": "./lib/packager-css.js",
    "./packager-html": "./lib/packager-html.js",
    "./packager-js": "./lib/packager-js.js",
    "./packager-raw-url": "./lib/packager-raw-url.js",
    "./packager-raw": "./lib/packager-raw.js",
    "./packager-svg": "./lib/packager-svg.js",
    "./packager-ts": "./lib/packager-ts.js",
    "./packager-wasm": "./lib/packager-wasm.js",
    "./packager-webextension": "./lib/packager-webextension.js",
    "./packager-xml": "./lib/packager-xml.js",
    "./plugin": {
      "default": "./lib/plugin.js",
      "types": "./types/core/plugin/src/PluginAPI.d.ts"
    },
    "./ProcessChild": "./ProcessChild.js",
    "./profiler": {
      "default": "./lib/profiler.js",
      "types": "./types/core/profiler/lib/index.d.ts"
    },
    "./reporter-build-metrics": "./lib/reporter-build-metrics.js",
    "./reporter-bundle-analyzer": "./lib/reporter-bundle-analyzer.js",
    "./reporter-bundle-buddy": "./lib/reporter-bundle-buddy.js",
    "./reporter-bundle-stats": "./lib/reporter-bundle-stats.js",
    "./reporter-cli": "./lib/reporter-cli.js",
    "./reporter-conditional-manifest": "./lib/reporter-conditional-manifest.js",
    "./reporter-dev-server": "./lib/reporter-dev-server.js",
    "./reporter-json": "./lib/reporter-json.js",
    "./reporter-sourcemap-visualiser": "./lib/reporter-sourcemap-visualiser.js",
    "./reporter-tracer": "./lib/reporter-tracer.js",
    "./resolver-default": "./lib/resolver-default.js",
    "./resolver-glob": "./lib/resolver-glob.js",
    "./runtime-browser-hmr": "./lib/runtime-browser-hmr.js",
    "./runtime-js": "./lib/runtime-js.js",
    "./runtime-react-refresh": "./lib/runtime-react-refresh.js",
    "./runtime-service-worker": "./lib/runtime-service-worker.js",
    "./runtime-webextension": "./lib/runtime-webextension.js",
    "./rust": {
      "default": "./lib/core/rust/index.js",
      "types": "./types/core/rust/index.d.ts"
    },
    "./service-worker": {
      "default": "./lib/service-worker.js",
      "types": "./types/utils/service-worker/service-worker.d.ts"
    },
    "./ThreadsChild": "./lib/ThreadsChild.js",
    "./transformer-babel": "./lib/transformer-babel.js",
    "./transformer-css": "./lib/transformer-css.js",
    "./transformer-glsl": "./lib/transformer-glsl.js",
    "./transformer-graphql": "./lib/transformer-graphql.js",
    "./transformer-html": "./lib/transformer-html.js",
    "./transformer-image": "./lib/transformer-image.js",
    "./transformer-inline-string": "./lib/transformer-inline-string.js",
    "./transformer-inline": "./lib/transformer-inline.js",
    "./transformer-js": "./lib/transformer-js.js",
    "./transformer-json": "./lib/transformer-json.js",
    "./transformer-jsonld": "./lib/transformer-jsonld.js",
    "./transformer-less": "./lib/transformer-less.js",
    "./transformer-mdx": "./lib/transformer-mdx.js",
    "./transformer-postcss": "./lib/transformer-postcss.js",
    "./transformer-posthtml": "./lib/transformer-posthtml.js",
    "./transformer-pug": "./lib/transformer-pug.js",
    "./transformer-raw": "./lib/transformer-raw.js",
    "./transformer-react-refresh-wrap": "./lib/transformer-react-refresh-wrap.js",
    "./transformer-svg-react": "./lib/transformer-svg-react.js",
    "./transformer-svg": "./lib/transformer-svg.js",
    "./transformer-toml": "./lib/transformer-toml.js",
    "./transformer-typescript-tsc": "./lib/transformer-typescript-tsc.js",
    "./transformer-typescript-types": "./lib/transformer-typescript-types.js",
    "./transformer-webextension": "./lib/transformer-webextension.js",
    "./transformer-webmanifest": "./lib/transformer-webmanifest.js",
    "./transformer-worklet": "./lib/transformer-worklet.js",
    "./transformer-xml": "./lib/transformer-xml.js",
    "./transformer-yaml": "./lib/transformer-yaml.js",
    "./ts-utils": "./lib/ts-utils.js",
    "./types-internal": {
      "default": "./lib/types-internal.js",
      "types": "./types/core/types-internal/lib/index.d.ts"
    },
    "./types": {
      "default": "./lib/types.js",
      "types": "./types/core/types/lib/index.d.ts"
    },
    "./utils": "./lib/utils.js",
    "./watcher-watchman-js": "./lib/watcher-watchman-js.js",
    "./worker": "./lib/worker.js",
    "./workers": {
      "default": "./lib/workers.js",
      "types": "./types/core/workers/index.d.ts"
    }
  },
  "sideEffects": true,
  "files": [
    "lib",
    "!lib/native/*win32*",
    "!lib/native/*musl*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "type": "commonjs"
}
