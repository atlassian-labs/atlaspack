{
  "name": "@atlaspack/super",
  "version": "2.14.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "build": "rm -rf lib && yarn generate-entries && yarn build:atlaspack && yarn create-empty-files",
    "generate-entries": "rm -rf entries && node ./scripts/generate-entries.js",
    "build:atlaspack": "NODE_ENV=production ATLASPACK_BUILD_ENV=production ATLASPACK_SUPER_BUILD=true yarn atlaspack build --no-cache --no-autoinstall ./entries/*.js --config .parcelrc",
    "create-empty-files": "touch lib/core/core/_empty.js && touch lib/utils/node-resolver-core/_empty.js"
  },
  "targets": {
    "default": {
      "context": "node",
      "isLibrary": true,
      "distDir": "lib",
      "optimize": false,
      "sourceMap": false,
      "includeNodeModules": {
        "lmdb": false
      }
    }
  },
  "alias": {
    "@swc/wasm": false,
    "@atlaspack/babel-register": false
  },
  "copyTypes": [
    "@atlaspack/cache",
    "@atlaspack/conditional-import-types",
    "@atlaspack/core",
    "@atlaspack/diagnostic",
    "@atlaspack/feature-flags",
    "@atlaspack/fs",
    "@atlaspack/package-manager",
    "@atlaspack/plugin",
    "@atlaspack/profiler",
    "@atlaspack/rust",
    "@atlaspack/types",
    "@atlaspack/types-internal",
    "@atlaspack/workers",
    "@mischnic/json-sourcemap",
    "chrome-trace-event",
    "@parcel/watcher",
    "@parcel/source-map"
  ],
  "devDependencies": {
    "@atlaspack/plugin": "*",
    "fast-glob": "^3.2.12",
    "prettier": "2.4.1"
  },
  "sideEffects": true,
  "files": [
    "lib",
    "!lib/native/*win32*",
    "!lib/native/*musl*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "type": "commonjs",
