{
  "name": "@atlaspack/cli",
  "version": "2.14.14",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "bin": {
    "atlaspack": "bin/atlaspack.js"
  },
  "main": "./lib/bin.js",
  "source": "./src/bin.ts",
  "types": "./src/bin.ts",
  "scripts": {
    "build:lib": "gulp build --gulpfile ../../../gulpfile.js --cwd ."
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "dependencies": {
    "@atlaspack/config-default": "workspace:*",
    "@atlaspack/core": "workspace:*",
    "@atlaspack/diagnostic": "workspace:*",
    "@atlaspack/events": "workspace:*",
    "@atlaspack/feature-flags": "workspace:*",
    "@atlaspack/fs": "workspace:*",
    "@atlaspack/logger": "workspace:*",
    "@atlaspack/package-manager": "workspace:*",
    "@atlaspack/reporter-cli": "workspace:*",
    "@atlaspack/reporter-dev-server": "workspace:*",
    "@atlaspack/reporter-tracer": "workspace:*",
    "@atlaspack/utils": "workspace:*",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "get-port": "^4.2.0"
  },
  "devDependencies": {
    "@atlaspack/babel-register": "workspace:*",
    "@babel/core": "^7.22.11",
    "rimraf": "^5.0.5"
  }
}
