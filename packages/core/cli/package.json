{
  "name": "@atlaspack/cli",
  "version": "2.13.15",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "bin": {
    "atlaspack": "bin/atlaspack.js"
  },
  "exports": {
    ".": {
      "@atlaspack::sources": "./src/cli-dev.mts",
      "types": "./src/cli.mts",
      "default": "./lib/cli.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "build-tsc": "yarn tsc --noCheck",
    "check-ts": "yarn tsc --noEmit"
  },
  "dependencies": {
    "@atlaspack/config-default": "3.1.13",
    "@atlaspack/core": "2.18.2",
    "@atlaspack/diagnostic": "2.14.1",
    "@atlaspack/events": "2.14.1",
    "@atlaspack/feature-flags": "2.17.0",
    "@atlaspack/fs": "2.15.6",
    "@atlaspack/logger": "2.14.10",
    "@atlaspack/package-manager": "2.14.11",
    "@atlaspack/reporter-cli": "2.15.7",
    "@atlaspack/reporter-dev-server": "2.14.11",
    "@atlaspack/reporter-tracer": "2.14.11",
    "@atlaspack/utils": "2.14.11",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "get-port": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@atlaspack/babel-register": "2.14.1"
  }
}
