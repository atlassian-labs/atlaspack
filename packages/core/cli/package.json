{
  "name": "@atlaspack/cli",
  "version": "2.12.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "main": "lib/bin.js",
  "source": "src/bin.ts",
  "types": "lib/bin.d.ts",
  "bin": {
    "atlaspack": "bin/atlaspack.js"
  },
  "scripts": {
    "dev:prepare": "rimraf ./lib/ && mkdir -p lib && cp ./bin/dev-bin.js ./lib/bin.js",
    "prepack": "./ensure-no-dev-lib.sh"
  },
  "dependencies": {
    "@atlaspack/config-default": "2.12.0",
    "@atlaspack/core": "2.12.0",
    "@atlaspack/diagnostic": "2.12.0",
    "@atlaspack/events": "2.12.0",
    "@atlaspack/feature-flags": "2.12.0",
    "@atlaspack/fs": "2.12.0",
    "@atlaspack/logger": "2.12.0",
    "@atlaspack/package-manager": "2.12.0",
    "@atlaspack/reporter-cli": "2.12.0",
    "@atlaspack/reporter-dev-server": "2.12.0",
    "@atlaspack/reporter-tracer": "2.12.0",
    "@atlaspack/utils": "2.12.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "get-port": "^4.2.0"
  },
  "devDependencies": {
    "@atlaspack/babel-register": "2.12.0",
    "@babel/core": "^7.22.11",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
