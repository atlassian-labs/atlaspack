{
  "name": "@atlaspack/cli",
  "version": "4.0.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "bin": {
    "atlaspack": "bin/atlaspack.js"
  },
  "main": "lib/bin.js",
  "source": "src/bin.js",
  "scripts": {
    "prepack": "./ensure-no-dev-lib.sh",
    "dev:prepare": "rimraf ./lib/ && mkdir -p lib && cp ./bin/dev-bin.js ./lib/bin.js"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "dependencies": {
    "@atlaspack/config-default": "4.0.0",
    "@atlaspack/core": "4.0.0",
    "@atlaspack/diagnostic": "2.14.1",
    "@atlaspack/events": "2.14.1",
    "@atlaspack/feature-flags": "4.0.0",
    "@atlaspack/fs": "4.0.0",
    "@atlaspack/logger": "4.0.0",
    "@atlaspack/package-manager": "4.0.0",
    "@atlaspack/reporter-cli": "4.0.0",
    "@atlaspack/reporter-dev-server": "4.0.0",
    "@atlaspack/reporter-tracer": "4.0.0",
    "@atlaspack/utils": "4.0.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "get-port": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@atlaspack/babel-register": "2.14.1",
    "rimraf": "^5.0.5"
  }
}
