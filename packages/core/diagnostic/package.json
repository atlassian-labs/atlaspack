{
  "name": "@atlaspack/diagnostic",
  "version": "2.14.1",
  "description": "Types and utilities for printing source-code located errors, warning and information messages.",
  "license": "(MIT OR Apache-2.0)",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.mts",
  "exports": {
    ".": {
      "atlaspack::sources": "./src/index.mts",
      "types": [
        "./lib/index.d.mts",
        "./src/index.mts"
      ],
      "import": "./lib/index.mjs",
      "module-sync": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build-tsc": "node ../../../scripts/build-tsc.mjs"
  },
  "dependencies": {
    "@mischnic/json-sourcemap": "^0.1.0",
    "nullthrows": "^1.1.1"
  }
}
