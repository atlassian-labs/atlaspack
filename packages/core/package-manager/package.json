{
  "name": "@atlaspack/package-manager",
  "version": "2.12.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "(MIT OR Apache-2.0)",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/atlaspack.git"
  },
  "main": "lib/index.js",
  "source": "src/index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build-ts": "mkdir -p lib && flow-to-ts src/index.js > lib/index.d.ts",
    "check-ts": "tsc --noEmit index.d.ts",
    "test": "mocha test"
  },
  "targets": {
    "types": false,
    "main": {
      "includeNodeModules": {
        "@atlaspack/core": false,
        "@atlaspack/diagnostic": false,
        "@atlaspack/fs": false,
        "@atlaspack/logger": false,
        "@atlaspack/node-resolver-core": false,
        "@atlaspack/types": false,
        "@atlaspack/utils": false,
        "@atlaspack/workers": false,
        "@swc/core": false,
        "semver": false
      }
    }
  },
  "dependencies": {
    "@atlaspack/diagnostic": "2.12.0",
    "@atlaspack/fs": "2.12.0",
    "@atlaspack/logger": "2.12.0",
    "@atlaspack/node-resolver-core": "3.3.0",
    "@atlaspack/types": "2.12.0",
    "@atlaspack/utils": "2.12.0",
    "@atlaspack/workers": "2.12.0",
    "@swc/core": "^1.7.26",
    "semver": "^7.5.2"
  },
  "devDependencies": {
    "command-exists": "^1.2.6",
    "cross-spawn": "^6.0.4",
    "nullthrows": "^1.1.1",
    "split2": "^3.1.1"
  },
  "peerDependencies": {
    "@atlaspack/core": "^2.12.0"
  },
  "browser": {
    "./src/NodePackageManager.js": false,
    "./src/Npm.js": false,
    "./src/Pnpm.js": false,
    "./src/Yarn.js": false
  }
}
