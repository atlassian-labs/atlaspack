{
  "name": "@atlaspack/ssr-app-example",
  "version": "2.12.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && atlaspack build src/index.js src/index2.js --target ssr",
    "viewer": "node server.js"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@atlaspack/cli": "2.15.10"
  },
  "targets": {
    "ssr": {
      "context": "tesseract",
      "distDir": "dist",
      "outputFormat": "commonjs",
      "__unstable_singleFileOutput": true
    }
  },
  "type": "commonjs"
}
