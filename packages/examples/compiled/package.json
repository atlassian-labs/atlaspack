{
  "name": "@atlaspack/compiled-css-in-js-example",
  "version": "0.1.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:inspect": "PARCEL_WORKERS=0 node --inspect-brk $(yarn bin atlaspack) build --no-cache src/index.html",
    "build": "atlaspack build --feature-flag enableTokensTransformer=true --feature-flag compiledCssInJsTransformer=true --feature-flag coreTokensAndCompiledCssInJsTransform=false --no-cache src/index.html",
    "dev": "npx nodemon -e 'ts, tsx, json, .parcelrc' --watch . --ignore 'dist/' --ignore 'compiled-css-migration-map.json' --exec 'yarn build && node serve.js'",
    "serve": "atlaspack serve --feature-flag enableTokensTransformer=true --feature-flag compiledCssInJsTransformer=true --feature-flag coreTokensAndCompiledCssInJsTransform=false --no-cache src/index.html"
  },
  "dependencies": {
    "@atlaskit/button": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@atlaspack/cli": "*",
    "@atlaspack/reporter-compiled-css-in-js-migration-map": "*",
    "@atlaspack/transformer-compiled-css-in-js": "*",
    "@types/react-dom": "^17.0.2",
    "express": "*"
  },
  "type": "commonjs",
  "@atlaspack/transformer-tokens": {
    "tokenDataPath": "../../../../atlassian-frontend-monorepo/platform/packages/design-system/tokens/src/artifacts/token-data.json5",
    "shouldUseAutoFallback": true,
    "shouldForceAutoFallback": false,
    "forceAutoFallbackExemptions": [
      "radius"
    ]
  }
}
