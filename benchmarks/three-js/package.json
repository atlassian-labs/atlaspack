{
  "name": "@atlaspack/benchmark-three-js",
  "private": true,
  "version": "0.1.2",
  "scripts": {
    "start": "node ./scripts/build.js",
    "start:v2": "env ATLASPACK_BENCH_MODE=V2 ATLASPACK_BENCH_PLUGINS=0 node ./scripts/build.mjs",
    "start:v2:plugins": "env ATLASPACK_BENCH_MODE=V2 ATLASPACK_BENCH_PLUGINS=10 node ./scripts/build.mjs",
    "start:v3": "env ATLASPACK_BENCH_MODE=V3 ATLASPACK_BENCH_PLUGINS=0 node ./scripts/build.mjs",
    "start:v3:plugins": "env ATLASPACK_BENCH_MODE=V3 ATLASPACK_BENCH_PLUGINS=10 node ./scripts/build.mjs"
  },
  "dependencies": {
    "@atlaspack/cli": "workspace:*",
    "@atlaspack/config-default": "workspace:*",
    "@atlaspack/core": "workspace:*",
    "@atlaspack/plugin": "workspace:*",
    "@oclif/table": "0.3.3",
    "@swc/helpers": "*",
    "chalk": "*"
  }
}
